{
  "name": "flower-delivery-app-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/",
    "preview": "vite preview"
